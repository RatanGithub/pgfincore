#!/bin/sh

PG_VER=`pg_config --version|awk '{print $2}'`
PG_VER=8.3.0

if [[ ($PG_VER < '8.4.0') ]]
then
  cp ./pgfincore_83.sql.in ./pgfincore.sql.in
  cp ./uninstall_pgfincore_83.sql.in ./uninstall_pgfincore.sql.in
else
  cp ./pgfincore_84.sql.in ./pgfincore.sql.in
  cp ./uninstall_pgfincore_84.sql.in ./uninstall_pgfincore.sql.in
fi

